                        {{#if git_url.length}}
                            <a href="{{git_url}}" style="text-decoration: none;">
                                <i class="bi bi-github"></i> view on GitHub
                            </a>
                        {{else}}
                            <p><i class="bi bi-github"></i> Private Repository</p>
                        {{/if}}
